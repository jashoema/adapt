# .env.example for action_executor agent
# Copy this file to .env and fill in your actual values

# SSH device username (never hardcode in scripts)
DEVICE_USERNAME=your_ssh_username

# SSH device password (never hardcode; load securely from environment!)
DEVICE_PASSWORD=your_ssh_password

# (Optional) Enable/privileged exec password
DEVICE_SECRET=your_enable_password

# Target device IP or hostname
DEVICE_HOSTNAME=192.0.2.100

# Device type (e.g., cisco_ios, juniper, arista_eos, etc. See Netmiko docs)
DEVICE_TYPE=cisco_ios

# SSH port (default 22)
DEVICE_PORT=22

# OPERATION MODE: 'true' for simulation, 'false' for real execution via SSH/Netmiko
SIMULATION_MODE=true